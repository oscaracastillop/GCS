@{
    ViewBag.Title = "Fondo Pensión";
}

<h7>
    <a href="/Home"><i>INICIO</i></a>
    &nbsp;<i class="bi-chevron-right"></i>&nbsp;
    <a href="/Modulo/Configuracion"><i>MODULO CONFIGURACIÓN</i></a>
    &nbsp;<i class="bi-chevron-right"></i>&nbsp;
    <a href="/FondoPension"><i>FONDO PENSION</i></a>
    &nbsp;<i class="bi-chevron-right"></i>&nbsp;
    <label style="font-weight:bold">CREAR FONDO PENSION</label>
    <hr />
</h7>

<table class="table table-borderless">
    <tr>
        <td>
            <label><strong>Nombre Fondo Pensión</strong></label>
            <input class="form-control" type="text" id="InputNombreFondoPension" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Email</strong></label>
            <input class="form-control" type="text" id="InputEmailFondoPension" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Teléfono</strong></label>
            <input class="form-control" type="text" id="InputTelefonoFondoPension" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <button class="btn btn-success btn-sm" onclick="CrearFondoPension()"><i class="bi-upload"></i> Guardar</button>
        </td>
    </tr>
</table>


