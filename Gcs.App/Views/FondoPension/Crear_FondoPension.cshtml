<table class="table table-borderless">
    <tr>
        <td>
            <label><strong>Nombre Fondo Cesantías</strong></label>
            <input class="form-control" type="text" id="InputNombreFondoCesantias" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Email</strong></label>
            <input class="form-control" type="text" id="InputEmailFondoCesantias" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Teléfono</strong></label>
            <input class="form-control" type="text" id="InputTelefonoFondoCesantias" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <button class="btn btn-success btn-sm" onclick="CrearFondoCesantias()"><i class="bi-upload"></i> Guardar</button>
        </td>
    </tr>
</table>


