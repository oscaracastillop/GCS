<table class="table table-borderless">
    <tr>
        <td>
            <label><strong>Nombre Eps</strong></label>
            <input class="form-control" type="text" id="InputNombreEps" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Email</strong></label>
            <input class="form-control" type="text" id="InputEmailEps" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <label><strong>Teléfono</strong></label>
            <input class="form-control" type="text" id="InputTelefonoEps" name="name" autocomplete="off" />
        </td>
    </tr>
    <tr>
        <td>
            <button class="btn btn-success btn-sm" onclick="CrearEps()"><i class="bi-upload"></i> Guardar</button>
        </td>
